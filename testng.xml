<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Framework1Suite" parallel="tests" thread-count="2">
	<listeners>
		<listener class-name="util.ReportUtil"></listener>
	</listeners>
	<test name="Simple form">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.SimpleFormTest"></class>
		</classes>
	</test>
	<test name="Input Test">

		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="Test.InputTest"></class>
		</classes>
	</test>

	<test name="Facebook test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.WindowPopUpTest"></class>
		</classes>
	</test>

	<test name="Javascript prompt Alert Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.JavascriptPromptVerifyTest"></class>
		</classes>
	</test>

	<test name="Form Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.FormSubmitTest"></class>
		</classes>
	</test>
	<test name="Form Error Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.SubmitFormErrorTest"></class>
		</classes>
	</test>
	<test name="Radio button Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.RadioButtonTest"></class>
		</classes>
	</test>
	<test name="JQuery Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.JQueryTest"></class>
		</classes>
	</test>

	<test name="Drag and Drop Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.DragAndDropTest"></class>
		</classes>
	</test>
	<test name="Table Verify Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="Test.TableDataVerifyTest"></class>
		</classes>
	</test>

</suite>